set_Nxx_dxx_invdx_params__and__xx.h:    params->Nxx_plus_2NGHOSTS0 = Nxx[0] + 2*NGHOSTS;
set_Nxx_dxx_invdx_params__and__xx.h:    params->Nxx_plus_2NGHOSTS1 = Nxx[1] + 2*NGHOSTS;
set_Nxx_dxx_invdx_params__and__xx.h:    params->Nxx_plus_2NGHOSTS2 = Nxx[2] + 2*NGHOSTS;
set_Nxx_dxx_invdx_params__and__xx.h:    params->dxx0 = (xxmax[0] - xxmin[0]) / ((REAL)Nxx[0]);
set_Nxx_dxx_invdx_params__and__xx.h:    params->dxx1 = (xxmax[1] - xxmin[1]) / ((REAL)Nxx[1]);
set_Nxx_dxx_invdx_params__and__xx.h:    params->dxx2 = (xxmax[2] - xxmin[2]) / ((REAL)Nxx[2]);
set_Nxx_dxx_invdx_params__and__xx.h:    params->invdx0 = 1.0/params->dxx0;
set_Nxx_dxx_invdx_params__and__xx.h:    params->invdx1 = 1.0/params->dxx1;
set_Nxx_dxx_invdx_params__and__xx.h:    params->invdx2 = 1.0/params->dxx2;
set_Nxx_dxx_invdx_params__and__xx.h:        xx[0][j] = xxmin[0] + ((REAL)(j-NGHOSTS) + (1.0/2.0))*params->dxx0; // Cell-centered grid.
set_Nxx_dxx_invdx_params__and__xx.h:        xx[1][j] = xxmin[1] + ((REAL)(j-NGHOSTS) + (1.0/2.0))*params->dxx1; // Cell-centered grid.
set_Nxx_dxx_invdx_params__and__xx.h:        xx[2][j] = xxmin[2] + ((REAL)(j-NGHOSTS) + (1.0/2.0))*params->dxx2; // Cell-centered grid.
